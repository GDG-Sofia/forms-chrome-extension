{
  "name": "Forms",
  "manifest_version": 2,
  "description": "Extension for managing forms",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "Forms"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
